{
  "package": {
    "productName": "calculator",
    "version": "1.6.0"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:8080",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "tauri": {
    "systemTray": {
      "iconPath": "./icons/icon.png",
      "iconAsTemplate": true
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.spada.calculator",
      "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {},
      "macOS": {},
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "shell": {
        "all": true,
        "scope": [
          {
            "name": "cmd",
            "cmd": "cmd",
            "args": ["/C", { "validator": "\\S+" }]
          },
          {
            "name": "pwsh",
            "cmd": "pwsh",
            "args": ["-Command", { "validator": "\\S+" }]
          }
        ]
      },
      "notification": {
        "all": true
      }
    },
    "windows": [
      {
        "title": "后期工资计算器",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
